	<queryString>
		<![CDATA[SELECT
     alumno.`id` AS alumno_id,
     alumno.`cedula` AS alumno_cedula,
     alumno.`apellidos` AS alumno_apellidos,
     alumno.`nombres` AS alumno_nombres,
     alumno.`direccion` AS alumno_direccion,
     alumno.`sexo` AS alumno_sexo,
     alumno.`fecha_nac` AS alumno_fecha_nac,
     alumno.`lug_nac` AS alumno_lug_nac,
     alumno.`vive_padres` AS alumno_vive_padres,
     alumno.`nacionalidad` AS alumno_nacionalidad,
     alumno.`observaciones` AS alumno_observaciones,
     alumno.`est_reg` AS alumno_est_reg,
     matricula.`id` AS matricula_id,
     matricula.`f_mat` AS matricula_f_mat,
     matricula.`est_reg` AS matricula_est_reg,
     matricula.`cant_doc_recib` AS matricula_cant_doc_recib,
     matricula.`id_alumno` AS matricula_id_alumno,
     matricula.`id_periodo` AS matricula_id_periodo,
     matricula.`id_paralelo` AS matricula_id_paralelo,
     paralelo.`id` AS paralelo_id,
     paralelo.`paralelo` AS paralelo_paralelo,
     paralelo.`est_reg` AS paralelo_est_reg,
     periodo.`id` AS periodo_id,
     periodo.`periodo` AS periodo_periodo,
     periodo.`est_reg` AS periodo_est_reg
FROM
     `alumno` alumno INNER JOIN `matricula` matricula ON alumno.`id` = matricula.`id_alumno`
     INNER JOIN `paralelo` paralelo ON matricula.`id_paralelo` = paralelo.`id`
     INNER JOIN `periodo` periodo ON matricula.`id_periodo` = periodo.`id`
WHERE
     matricula.`id` = $P!{n_mat}]]>
	</queryString>
