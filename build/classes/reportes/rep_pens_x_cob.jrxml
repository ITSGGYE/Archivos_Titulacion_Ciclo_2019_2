	<queryString>
		<![CDATA[SELECT
     pag_pend.`id` AS pag_pend_id,
     pag_pend.`id_matricula` AS pag_pend_id_matricula,
     pag_pend.`id_transaccion` AS pag_pend_id_transaccion,
     pag_pend.`valor` AS pag_pend_valor,
     pag_pend.`saldo` AS pag_pend_saldo,
     pag_pend.`cancelado` AS pag_pend_cancelado,
     pag_pend.`est_reg` AS pag_pend_est_reg,
     matricula.`id` AS matricula_id,
     matricula.`f_mat` AS matricula_f_mat,
     matricula.`h_mat` AS matricula_h_mat,
     matricula.`est_reg` AS matricula_est_reg,
     matricula.`cant_doc_recib` AS matricula_cant_doc_recib,
     matricula.`edad` AS matricula_edad,
     matricula.`id_alumno` AS matricula_id_alumno,
     matricula.`id_periodo` AS matricula_id_periodo,
     matricula.`id_paralelo` AS matricula_id_paralelo,
     transaccion.`id` AS transaccion_id,
     transaccion.`transaccion` AS transaccion_transaccion,
     transaccion.`est_reg` AS transaccion_est_reg,
     alumno.`id` AS alumno_id,
     alumno.`cedula` AS alumno_cedula,
     alumno.`apellidos` AS alumno_apellidos,
     alumno.`nombres` AS alumno_nombres,
     periodo.`id` AS periodo_id,
     periodo.`periodo` AS periodo_periodo,
     periodo.`est_reg` AS periodo_est_reg,
     paralelo.`id` AS paralelo_id,
     paralelo.`paralelo` AS paralelo_paralelo
FROM
     `matricula` matricula INNER JOIN `pag_pend` pag_pend ON matricula.`id` = pag_pend.`id_matricula`
     INNER JOIN `transaccion` transaccion ON pag_pend.`id_transaccion` = transaccion.`id`
     INNER JOIN `alumno` alumno ON matricula.`id_alumno` = alumno.`id`
     INNER JOIN `periodo` periodo ON matricula.`id_periodo` = periodo.`id`
     INNER JOIN `paralelo` paralelo ON matricula.`id_paralelo` = paralelo.`id`
WHERE pag_pend.`id_transaccion`=$P!{cod_transaccion}]]>
	</queryString>
