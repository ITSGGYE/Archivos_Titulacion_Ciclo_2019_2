	</parameter>
	<queryString>
		<![CDATA[SELECT
     matricula.`id` AS matricula_id,
     matricula.`f_mat` AS matricula_f_mat,
     matricula.`est_reg` AS matricula_est_reg,
     matricula.`cant_doc_recib` AS matricula_cant_doc_recib,
     matricula.`id_alumno` AS matricula_id_alumno,
     matricula.`id_periodo` AS matricula_id_periodo,
     matricula.`id_paralelo` AS matricula_id_paralelo,
     paralelo.`id` AS paralelo_id,
     paralelo.`paralelo` AS paralelo_paralelo,
     paralelo.`est_reg` AS paralelo_est_reg,
     alumno.`id` AS alumno_id,
     alumno.`cedula` AS alumno_cedula,
     alumno.`apellidos` AS alumno_apellidos,
     alumno.`nombres` AS alumno_nombres,
     alumno.`direccion` AS alumno_direccion,
     alumno.`sexo` AS alumno_sexo,
     alumno.`fecha_nac` AS alumno_fecha_nac,
     alumno.`lug_nac` AS alumno_lug_nac,
     alumno.`vive_padres` AS alumno_vive_padres,
     alumno.`nacionalidad` AS alumno_nacionalidad,
     alumno.`observaciones` AS alumno_observaciones,
     alumno.`est_reg` AS alumno_est_reg,
     rel_matri_doc.`id` AS rel_matri_doc_id,
     rel_matri_doc.`id_matricula` AS rel_matri_doc_id_matricula,
     rel_matri_doc.`id_documento` AS rel_matri_doc_id_documento,
     rel_matri_doc.`entregado` AS rel_matri_doc_entregado,
     rel_matri_doc.`est_reg` AS rel_matri_doc_est_reg,
     documento.`id` AS documento_id,
     documento.`documento` AS documento_documento,
     documento.`est_reg` AS documento_est_reg
FROM
     `paralelo` paralelo INNER JOIN `matricula` matricula ON paralelo.`id` = matricula.`id_paralelo`
     INNER JOIN `alumno` alumno ON matricula.`id_alumno` = alumno.`id`
     INNER JOIN `rel_matri_doc` rel_matri_doc ON matricula.`id` = rel_matri_doc.`id_matricula`
     INNER JOIN `documento` documento ON rel_matri_doc.`id_documento` = documento.`id`
WHERE
     matricula.`id_paralelo` = $P!{cd_prl}
 AND rel_matri_doc.`entregado` = 'N'
 AND rel_matri_doc.`est_reg` = 'A'
 AND matricula.`est_reg` = 'A']]>
	</queryString>
