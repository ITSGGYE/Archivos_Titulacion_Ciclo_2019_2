  `pla_planificacion_union`.`id_planificacion_union`,`pla_planificacion_union`.`id_usuario`, 
  `pla_planificacion_union`.`u_actualizacion`,`pla_planificacion_union`.`u_creacion`, 
  `pla_planificacion`.`id_planificacion`,`pla_planificacion`.`fecha`, 
  `pla_planificacion`.`hora`,`pla_planificacion`.`objetivo_unidad`,`pla_planificacion`.`criterio_evaluacion`,
  `pla_planificacion`.`destresa_criterio_desempeno`,`pla_planificacion`.`actividades_aprendizaje`,
  `pla_planificacion`.`recursos`,`pla_planificacion`.`tecnicas_instrumentos_evaluacion`,
  `pla_planificacion`.`especificaciones_unidad_educativa`,`pla_planificacion`.`especificaciones_adaptacion_aplicada`,
  `pla_planificacion`.`revisado`,`pla_planificacion`.`aprovado`,`pla_planificacion`.`evaluacion_unidad`, 
  
  `ca_materia`.`materia`,`ma_paralelo`.`paralelo`,`ma_periodo`.`periodo`,`pla_objetivos`.`objetivos`, 
  `pla_objetivos`.`descripcion`, 
  
  `us_usuario`.`usuario`,`us_rol`.`id_rol`,`us_rol`.`rol`,`us_registro`.`id_registro`,`us_registro`.`apellidos_nombres`, 
  
  `em_sucursal`.`id_sucursal`,`em_sucursal`.`nombre_comercial_su` 
  
FROM `pla_planificacion` 
INNER JOIN `pla_planificacion_union` ON `pla_planificacion`.`id_planificacion` = `pla_planificacion_union`.`id_planificacion` 
INNER JOIN `ca_materia` ON `ca_materia`.`id_materia` = `pla_planificacion_union`.`id_materia` 
INNER JOIN `ma_paralelo` ON `ma_paralelo`.`id_paralelo` = `pla_planificacion_union`.`id_paralelo` 
INNER JOIN `ma_periodo` ON `ma_periodo`.`id_periodo` = `pla_planificacion_union`.`id_periodo` 
INNER JOIN `pla_objetivos` ON `pla_objetivos`.`id_objetivos` = `pla_planificacion_union`.`id_objetivo` 
INNER JOIN `us_usuario` ON `us_usuario`.`id_usuario` = `pla_planificacion_union`.`id_usuario` 
INNER JOIN `us_rol` ON `us_rol`.`id_rol` = `us_usuario`.`id_rol` 
INNER JOIN `us_registro` ON `us_registro`.`id_registro` = `us_usuario`.`id_registro` 
INNER JOIN `em_sucursal` ON `em_sucursal`.`id_sucursal` = `us_usuario`.`id_sucursal`
WHERE `pla_planificacion_union`.`id_planificacion` = 1 ;]]>
